  cX.callQueue.push(['invoke', function() {
    var cp = {}, sa = {};
    var cpMaxLen = 40;
    var cpKeyLen = 20;
    try{
      var refhost = cX.library._findRealReferrer().split('/')[2];
      if(refhost && location.hostname !== refhost) {
        cp["referrer_host"] = sa["referrer_host_S"] = refhost.slice(0,cpMaxLen);
      }
    }catch(e){}
