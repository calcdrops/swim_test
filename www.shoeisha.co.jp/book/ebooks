          if (s > 33) {
              $(".navbar-fixed-top").addClass("navbar-fixed-top-bg1");
              $(".navbar-fixed-top input.header-search").addClass("navbar-fixed-top-bg2");
              $(".navbar-fixed-top .nav > li > a").addClass("navbar-fixed-top-bg3");
